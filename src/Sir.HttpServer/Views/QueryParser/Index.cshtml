@model IEnumerable<Sir.HttpServer.Controllers.SearchResultModel>

@{
    ViewData["Title"] = "Go.go!";
    Layout = "~/Views/_Layout.cshtml";

    IList<Sir.HttpServer.Controllers.SearchResultModel> docs = Model == null ?
        new Sir.HttpServer.Controllers.SearchResultModel[0].ToList() : Model.ToList();
}
@{await Html.RenderPartialAsync("QueryParserForm");}

<p>API documentation: <a href="https://github.com/kreeben/resin/blob/master/README.md#create-your-own-collection">Read, write, query and slice collections</a></p>

@if (ViewData["time_ms"] != null)
{
    await Html.RenderPartialAsync("DocumentList", docs);
}