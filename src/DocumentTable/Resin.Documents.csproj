<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>3.0.0</Version>
    <Authors>Marcus Lager</Authors>
    <Company>Marcus Lager</Company>
    <Description>Map and compress variable sized business entities into fixed-sized bitmaps.</Description>
    <Copyright>Copyright 2017 Marcus Lager</Copyright>
    <PackageLicenseUrl>https://github.com/kreeben/resin/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/kreeben/resin</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kreeben/resin</RepositoryUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageId>DocumentTable</PackageId>
    <Product>DocumentTable</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="log4net" Version="2.0.8" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\StreamIndex\Resin.Bitmap.csproj" />
  </ItemGroup>

